    <j.0:isWelcomedBy rdf:resource="http://decodes/resources/company_CNAM_397"/>
    <j.0:isSubmittedBy rdf:resource="http://decodes/resources/respo_CNAM_397"/>
    <j.0:Name>Analyse des images et des sons numériques</j.0:Name>
