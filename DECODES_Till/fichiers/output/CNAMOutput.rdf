  <rdf:Description rdf:about="http://decodes/resources/formation_CNAM_139">
    <j.0:takesPlaceIn>
      <rdf:Description rdf:about="http://decodes/resources/location_CNAM_139">
        <j.0:Address>Paris75003Paris</j.0:Address>
