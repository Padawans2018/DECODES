    <j.0:isManagedBy rdf:resource="http://decodes/resources/contact_CNAM_436"/>
    <j.0:isSubmittedBy rdf:resource="http://decodes/resources/respo_CNAM_436"/>
    <j.0:Name>Ingénierie de la fouille et de la visualisation de données massives</j.0:Name>
  </rdf:Description>
