    <j.0:Name>Licence professionnelle Intervention sociale : accompagnement social Parcours Coordinateur(trice) de projets de développement d'économie sociale et solidaire</j.0:Name>
    <j.0:givesOpportunitiesIn rdf:resource="http://decodes/resources/job_stEtienne_0"/>
    <j.0:isPartOf>
